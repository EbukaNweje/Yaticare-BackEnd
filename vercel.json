{
  "name": "Yaticare-Backend",
  "version": 2,

  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },

  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ]
}
